<p id="notice"><%= notice %></p>

<h1>Articles Controllers</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @articles_controllers.each do |articles_controller| %>
      <tr>
        <td><%= link_to 'Show', articles_controller %></td>
        <td><%= link_to 'Edit', edit_articles_controller_path(articles_controller) %></td>
        <td><%= link_to 'Destroy', articles_controller, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Articles Controller', new_articles_controller_path %>
